// NFA Visualization
digraph {
	graph [rankdir=LR]
	start [label=start shape=plaintext]
	S0 [label=S0 shape=doublecircle]
	start -> S0
	S0 -> S0 [label=0]
	S1 [label=S1 shape=circle]
	S0 -> S1 [label=1]
	S2 [label=S2 shape=doublecircle]
	S1 -> S2 [label=0]
	S1 -> S0 [label=1]
	S2 -> S1 [label=0]
	S2 -> S2 [label=1]
}
